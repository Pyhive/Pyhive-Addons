{
    "name": "Elk",
    "version": "dev",
    "slug": "elk",
    "description": "AWS ELK for storing events",
    "panel_icon": "mdi:aws",
    "startup": "services",
    "homeassistant": "2021.4.0",
    "arch": [
        "amd64",
        "armhf",
        "armv7"
    ],
    "init": false,
    "ports": {
        "5044/tcp": 5044,
        "5601/tcp": 5601,
        "9200/tcp": 9200,
        "9300/tcp": 9300,
        "9600/tcp": 9600
    },
    "ports_description": {
        "9200/tcp": "DNS server port",
        "9300/tcp": "Web interface (Not required for Ingress)"
    },
    "auth_api": true,
    "host_network": true,
    "hassio_api": true,
    "hassio_role": "manager",
    "map": [
        "ssl"
    ],
    "options": {
        "ssl": true,
        "certfile": "fullchain.pem",
        "keyfile": "privkey.pem"
    },
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
        "ssl": "bool",
        "certfile": "str",
        "keyfile": "str",
        "leave_front_door_open": "bool?"
    }
}