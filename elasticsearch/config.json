{
    "name": "Elastic Search",
    "version": "dev",
    "slug": "elasticsearch",
    "description": "AWS Elastic search for storing events",
    "panel_icon": "mdi:aws",
    "startup": "services",
    "homeassistant": "2021.4.0",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "init": false,
    "ports": {
        "9200/tcp": 9200,
        "9300/tcp": 9300
    },
    "ports_description": {
        "9200/tcp": "DNS server port",
        "9300/tcp": "Web interface (Not required for Ingress)"
    },
    "auth_api": true,
    "host_network": true,
    "hassio_api": true,
    "hassio_role": "manager",
    "map": [
        "ssl"
    ],
    "options": {
        "ssl": true,
        "certfile": "fullchain.pem",
        "keyfile": "privkey.pem"
    },
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
        "ssl": "bool",
        "certfile": "str",
        "keyfile": "str",
        "leave_front_door_open": "bool?"
    },
    "image": "docker.elastic.co/elasticsearch/elasticsearch:7.12.1"
}