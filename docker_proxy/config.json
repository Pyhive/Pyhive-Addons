{
    "name": "Docker Proxy",
    "version": "dev",
    "slug": "docker_proxy",
    "description": "Manage your Docker environment with ease",
    "advanced": true,
    "startup": "services",
    "homeassistant": "2021.4.0",
    "arch": [
        "armhf",
        "armv7"
    ],
    "init": false,
    "map": [
        "ssl"
    ],
    "ports": {
        "2375/tcp": null
    },
    "ports_description": {
        "2375/tcp": "Agent Api to get docker info."
    },
    "hassio_api": true,
    "docker_api": true,
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)?"
    }
}