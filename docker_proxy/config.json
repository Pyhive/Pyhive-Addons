{
    "name": "Docker Proxy",
    "version": "0.01",
    "slug": "docker_proxy",
    "description": "Manage your Docker environment with docker proxy",
    "url": "https://github.com/Pyhass/Addons",
    "advanced": true,
    "ingress": true,
    "ingress_port": 0,
    "startup": "services",
    "panel_icon": "mdi:docker",
    "homeassistant": "0.92.0b2",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7"
    ],
    "init": false,
    "map": [
        "ssl"
    ],
    "hassio_api": true,
    "docker_api": true,
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)?"
    }
}